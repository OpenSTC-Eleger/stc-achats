<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    
	<record model="ir.rule" id="open_engagement_rule_all_access_specific">
        <field name="name">Engagements Rule Access</field>
        <field name="model_id" ref="model_open_engagement"/>
        <field name="domain_force">[('user_id','=',user.id)]</field>
        <field eval="[(6,0,[ref('openstc.openstc_employee'),ref('openstc.openstc_qualifier'),ref('openstc.openstc_agent')])]" name="groups"/>
        <field eval="1" name="perm_read"/>
        <field eval="1" name="perm_create"/>
        <field eval="1" name="perm_write"/>
        <field eval="1" name="perm_unlink"/>
    </record>
	
	<record model="ir.rule" id="open_achat_stock_account_analytic_all_access_specific">
        <field name="name">Comptes Analytiques Rule Access</field>
        <field name="model_id" ref="model_account_analytic_account"/>
        <field name="domain_force">[('service_id','in',[s.id for s in user.service_ids] )]</field>
        <field eval="[(6,0,[ref('openstc.openstc_employee'),ref('openstc.openstc_qualifier'),ref('openstc.openstc_agent'),
            ref('openstc.openstc_dst'),ref('openstc_elu'),ref('openstc.openstc_manager')])]" name="groups"/>
        <field eval="1" name="perm_read"/>
        <field eval="1" name="perm_create"/>
        <field eval="1" name="perm_write"/>
        <field eval="1" name="perm_unlink"/>
    </record>
	
	
</data>
</openerp>
