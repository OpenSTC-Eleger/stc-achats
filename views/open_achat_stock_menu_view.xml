<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	    
	    <record id="action_open_achat_stock_reception_picking_move" model="ir.actions.act_window">
            <field name="name">Réception par Produits</field>
            <field name="res_model">stock.move</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">['|','&amp;',('picking_id','=',False),('location_id.usage', 'in', ['customer','supplier']),'&amp;',('picking_id','!=',False),('picking_id.type','=','in'),('name','not ilike','%emprunt%')]</field>
            <field name="view_id" ref="stock.view_move_tree_reception_picking"/>
            <field name="context" eval="'{\'search_default_receive\':1, \'search_default_available\':1, \'product_receive\' : True, \'default_location_id\':%d, \'default_location_dest_id\':%d}' % (ref('stock.stock_location_suppliers'),ref('stock.stock_location_stock') )"/>
            <field name="search_view_id" ref="stock.view_move_search_reception_incoming_picking"/>
            <field name="help">Here you can receive individual products, no matter what purchase order or picking order they come from. You will find the list of all products you are waiting for. Once you receive an order, you can filter based on the name of the supplier or the purchase order reference. Then you can confirm all products received using the buttons on the right of each line.</field>
        </record>
	    
	    <record id="action_view_po_ask" model="ir.actions.act_window">
            <field name="name">Marchés A Bons de Commandes</field>
            <field name="res_model">purchase.order.ask</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
	    
	    <record id="action_view_draft_purchase" model="ir.actions.act_window">
            <field name="name">Commandes soumises à Validation</field>
            <field name="res_model">purchase.order</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('state','=','draft')]</field>
        </record>
	    
	    <record id="action_view_valid_purchase" model="ir.actions.act_window">
            <field name="name">Commandes Validés</field>
            <field name="res_model">purchase.order</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('state','&lt;&gt;','draft')]</field>
        </record>
        
	    <record id="action_view_engagements" model="ir.actions.act_window">
            <field name="name">Voir Engagements</field>
            <field name="res_model">open.engagement</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_in_progress':'1'}</field>
        </record>
	    
	    <record id="openstc_achat_stock_action_view_products" model="ir.actions.act_window">
            <field name="name">Voir Fournitures</field>
            <field name="res_model">product.product</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('active','=',True),('purchase_ok','=',True),('isroom','=',False)]</field>
        </record>
	    
	    <record id="openstc_ask_prod_action_view" model="ir.actions.act_window">
            <field name="name">Demandes de Fournitures</field>
            <field name="res_model">openstc.ask.prod</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
	    
	    
	    <menuitem id="menu_open_achat_stock" name="Achats-Stocks" sequence="10"/>
	    
	    <menuitem id="menu_open_achat_stock_stocks" name="Stocks" sequence="1" parent="menu_open_achat_stock"/>
	    <menuitem id="menu_open_achat_stock_achats" name="Achats" sequence="2" parent="menu_open_achat_stock"/>
	    <menuitem id="menu_open_achat_stock_budgets" name="Budgets" sequence="3" parent="menu_open_achat_stock"/>
	    
	    <menuitem id="menu_open_achat_stock_stock_view_picking_move" name="Récéption par Produits"
	        sequence="1" parent="menu_open_achat_stock_stocks" action="action_open_achat_stock_reception_picking_move"/>
	    <menuitem id="menu_open_achat_stock_stock_view_inventory_form" name="Inventaire Physique"
	        sequence="2" parent="menu_open_achat_stock_stocks" action="stock.action_inventory_form"/>
	    <menuitem id="menu_open_achat_stock_stock_view_product" name="Fournitures"
	        sequence="3" parent="menu_open_achat_stock_stocks" action="openstc_achat_stock_action_view_products"/>
	    <menuitem id="menu_open_achat_stock_stock_view_ask_prod"
	        sequence="" parent="menu_open_achat_stock_stocks" action="openstc_ask_prod_action_view"/>
		
		<menuitem id="menu_open_achat_stock_stock_view_supplier" name="Fournisseurs"
	        sequence="3" parent="menu_open_achat_stock_stocks" action="base.action_partner_supplier_form"/>
		
		<menuitem id="menu_open_achat_stock_achat_po_ask"
	        sequence="1" parent="menu_open_achat_stock_achats" action="action_view_po_ask"/>
	    <menuitem id="menu_open_achat_stock_stock_view_draft_purchase" name="Commandes soumises à Validation"
	        sequence="2" parent="menu_open_achat_stock_achats" action="action_view_draft_purchase"/>
		<menuitem id="menu_open_achat_stock_achats_view_purchase"
	        sequence="3" parent="menu_open_achat_stock_achats" action="action_view_valid_purchase"/>
		<menuitem id="menu_open_achat_stock_view_engagements" name="Engagements"
	        sequence="4" parent="menu_open_achat_stock_achats" action="action_view_engagements"/>
		<menuitem id="menu_open_achat_stock_achats_view_invoice" name="Factures"
	        sequence="5" parent="menu_open_achat_stock_achats" action="account.action_invoice_tree"/>
		
	    <menuitem id="menu_open_achat_stock_stock_view_budgets" name="Budgets"
	        sequence="1" parent="menu_open_achat_stock_budgets" action="account_budget.act_crossovered_budget_view"/>
	    
	</data>
</openerp>