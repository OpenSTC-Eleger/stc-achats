<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
	    <record model="ir.ui.view" id="open_engage_check_elu_wizard_form">
			<field name="name">open.engagement.check.elu.wizard.form</field>
			<field name="model">open.engagement.check.elu.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Donner une note explicative pour votre Autorisation">
				    <label string="Please explain your acceptation" colspan="4"/>
				    <field name="justif_check" colspan="4" nolabel="1"/>
				    <button special="cancel" string="Anuler" icon="gtk-cancel"/>
				    <button name="check_elu" type="object" string="Valider" icon="gtk-go-forward"/>
				</form>
			</field>
		</record>
	    
		<record model="ir.ui.view" id="openstc_view_service_site_cost_wizard">
			<field name="name">openstc.report.service.site.cost.wizard</field>
			<field name="model">openstc.report.service.site.cost.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<label
					string="Cliquez sur Imprimer pour visualiser les Couts des Sites du début d'année A aujourd'hui." />
				<newline />
				<button special="cancel" string="Annuler" icon="gtk-cancel" />
				<button name="print_report" type="object" string="Imprimer"
					icon="gtk-go-forward" />
			</field>
		</record>

		<!-- <record id="action_openstc_pret_view_checkout" model="ir.actions.act_window"> 
